    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@sanity/image-url": "^1.1.0",
    "@sanity/structure": "^2.36.2",
    "@sanity/ui": "^2.11.7",
    "@sanity/vision": "^3.71.2",
    "dayjs": "^1.11.13",
    "next-sanity": "^9.8.46",
    "react-is": "^19.0.0",
    "sanity": "^3.71.2",
